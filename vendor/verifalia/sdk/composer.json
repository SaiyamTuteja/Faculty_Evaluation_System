  "homepage": "https://verifalia.com/",
  "keywords": [
    "verifalia",
    "email",
    "validation",
    "verification",
    "free",
    "service",
    "saas",
    "list",
    "hygiene",
    "cleaning",
    "mx",
    "smtp",
    "mailbox",
    "scrub"
  ],
  "authors": [
