      "name": "Rudy Chiappetta"
    },
    {
      "name": "John Grounz"
    },
    {
      "name": "Efran Cobisi"
    },
    {
      "name": "Stefano Tatah"
    },
    {
      "name": "Germano Mosconi"
    },
    {
      "name": "Alex Soffiateli"
    }
  ],
  "autoload": {
    "psr-4": {
      "Verifalia\\": "lib/Verifalia"
    }
  },
  "require": {
    "php": ">=7.0",
    "guzzlehttp/guzzle": "~6.0",
    "ext-json": "*"
  },
  "support": {
